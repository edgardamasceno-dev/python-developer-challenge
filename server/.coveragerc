[run]
source = src
omit = 
    */tests/*
    */venv/*
    */__init__.py
    src/main.py
    src/core/logging.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:

[html]
directory = htmlcov